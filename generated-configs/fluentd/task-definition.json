{
	"family": "firelens-example-fluentd",
	"networkMode": "bridge",
	"executionRoleArn": "arn:aws:iam::144718711470:role/ecsExecutionRole",
	"taskRoleArn": "arn:aws:iam::144718711470:role/ecs_task_role",
	"cpu": "512",
	"memory": "1024",
	"containerDefinitions": [
		{
			"essential": true,
			"image": "144718711470.dkr.ecr.ap-south-1.amazonaws.com/awsfluentd",
			"name": "log_router",
			"firelensConfiguration": {
				"type": "fluentd",
				"options": {
					"config-file-type": "s3",
					"config-file-value": "arn:aws:s3:::firehose-fluent-output-bucket-bom/fluentd.conf"
				}
			}
		 },
		 {
			 "essential": true,
			 "image": "144718711470.dkr.ecr.us-west-2.amazonaws.com/better-json-logger:latest",
			 "name": "app",
			 "logConfiguration": {
				 "logDriver":"awsfirelens",
				 "options": {
					"@type": "kinesis_firehose",
					"delivery_stream_name": "demo-stream",
					"region": "ap-south-1"
				}
			}
		}
	]
}
